<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:mapbox="http://schemas.android.com/apk/res-auto"
                                             android:layout_width="match_parent"
                                             android:layout_height="match_parent">


    <com.mapbox.mapboxsdk.maps.MapView
            android:id="@+id/mapView"
            mapbox:mapbox_cameraTargetLat = "36.862499"
            mapbox:mapbox_cameraTargetLng = "10.195556"
            mapbox:mapbox_styleUrl="mapbox://styles/mapbox/streets-v10"
            mapbox:mapbox_cameraZoom = "12"
            android:layout_height="330dp"
            android:layout_width="match_parent"
            android:layout_marginTop="8dp"
            mapbox:layout_constraintTop_toTopOf="parent"
            mapbox:layout_constraintEnd_toEndOf="parent"
            mapbox:layout_constraintStart_toStartOf="parent" />
    <Button
            android:text="toEtape2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/toEtape2" android:layout_marginTop="8dp"
            mapbox:layout_constraintTop_toBottomOf="@+id/mapView"
            mapbox:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="16dp"
            mapbox:layout_constraintStart_toStartOf="parent" android:layout_marginStart="16dp"/>


</android.support.constraint.ConstraintLayout>